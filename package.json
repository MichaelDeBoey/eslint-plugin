{
  "name": "@mysticatea/eslint-plugin",
  "version": "8.0.1",
  "description": "Additional ESLint rules.",
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "index.js",
  "files": [
    "lib"
  ],
  "peerDependencies": {
    "eslint": ">=5.3.0"
  },
  "dependencies": {
    "eslint-plugin-eslint-comments": "~3.0.1",
    "eslint-plugin-eslint-plugin": "~2.0.0",
    "eslint-plugin-node": "~8.0.0",
    "eslint-plugin-prettier": "~3.0.0",
    "eslint-plugin-typescript": "~0.14.0",
    "eslint-plugin-vue": "~5.0.0",
    "prettier": "~1.14.3",
    "typescript-eslint-parser": "^21.0.2",
    "vue-eslint-parser": "^4.0.3"
  },
  "devDependencies": {
    "@mysticatea/eslint-plugin": "^7.0.0",
    "chokidar-cli": "^1.2.1",
    "codecov": "^3.1.0",
    "eslint": "~5.10.0",
    "fs-extra": "^7.0.1",
    "globals": "^11.9.0",
    "mocha": "^5.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^13.1.0",
    "opener": "^1.5.1",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "preversion": "run-s clean update test",
    "version": "eslint lib/rules --fix && git add lib",
    "postversion": "git push && git push --tags",
    "clean": "rimraf .nyc_output coverage",
    "lint": "eslint lib scripts tests",
    "pretest": "npm run -s lint",
    "test": "nyc mocha \"tests/lib/**/*.js\" --reporter dot",
    "update": "node scripts/generate-browser-globals && node scripts/generate-configs && node scripts/generate-rules",
    "watch": "chokidar lib tests --initial --command \"npm test && nyc report --reporter lcov\"",
    "coverage": "nyc report --reporter lcov && opener coverage/lcov-report/index.html",
    "codecov": "nyc report --reporter lcovonly && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mysticatea/eslint-plugin.git"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "block",
    "scope",
    "instanceof",
    "isArray",
    "static",
    "this",
    "rest",
    "spread",
    "ignore"
  ],
  "author": "Toru Nagashima",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mysticatea/eslint-plugin/issues"
  },
  "homepage": "https://github.com/mysticatea/eslint-plugin#readme"
}
